global ((int, int) -> int) op;

int lshift() {
  return (1 << 0) == 1;
}

int rshift() {
  return (1 >> 0) == 1;
}

int lshift_var(int i) {
  return (1 << i) == 1;
}

int rshift_var(int i) {
  return (1 >> i) == 1;
}

int main(int x) {
  if(x == 0) {
    return lshift();
  } elseif (x == 1) {
    return rshift();
  }  elseif (x == 2) {
    return rshift_var(0);
  }  elseif (x == 3) {
    return lshift_var(0);
  }  elseif (x == 4) {
    return rshift_var(1);
  }  else {
    return lshift_var(1);
  }
}

